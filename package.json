{
  "name": "wdio-electron-service",
  "version": "3.5.0",
  "description": "WebdriverIO service to enable Electron testing",
  "author": "Sam Maister <goosewobbler@protonmail.com>",
  "homepage": "https://github.com/webdriverio-community/wdio-electron-service",
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "type": "module",
  "module": "./dist/index.js",
  "exports": {
    "./preload": [
      {
        "import": "./dist/preload.js",
        "require": "./dist/cjs/preload.js"
      },
      "./dist/cjs/preload.js"
    ],
    "./main": [
      {
        "import": "./dist/main.js",
        "require": "./dist/cjs/main.js"
      },
      "./dist/cjs/main.js"
    ],
    ".": [
      {
        "import": "./dist/index.js",
        "require": "./dist/cjs/index.js"
      },
      "./dist/cjs/index.js"
    ]
  },
  "typeScriptVersion": "4.9.4",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": "^16.13 || >=18"
  },
  "scripts": {
    "init": "npm install -g pnpm && pnpm i",
    "prepare": "husky install",
    "ci": "pnpm build && pnpm test && pnpm lint",
    "clean": "rm -rf ./node_modules pnpm-lock.yaml ./dist",
    "clean:dist": "rm -rf ./dist",
    "build": "tsc -b .",
    "lint": "eslint --ignore-path .eslintignore \"**/*.{j,t}s\" && prettier --check \"**/*.{j,t}s\"",
    "lint:fix": "eslint --ignore-path .eslintignore \"**/*.{j,t}s\" --fix",
    "format": "prettier --write \"**/*.{j,t}s\"",
    "release": "release-it --github.release",
    "release:ci": "pnpm release --ci --npm.skipChecks --no-git.requireCleanWorkingDir",
    "release:patch": "pnpm release patch",
    "release:minor": "pnpm release minor",
    "release:major": "pnpm release major",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:watch-coverage": "jest --watch --coverage",
    "test": "pnpm test:coverage",
    "test:integration": "pnpm build && cd example && pnpm clean && pnpm i && pnpm build && pnpm t"
  },
  "keywords": [
    "webdriverio",
    "wdio",
    "wdio-service",
    "electron",
    "chromedriver",
    "tests"
  ],
  "peerDependencies": {
    "electron": "*",
    "electron-chromedriver": "*"
  },
  "peerDependenciesMeta": {
    "electron": {
      "optional": true
    },
    "electron-chromedriver": {
      "optional": true
    }
  },
  "dependencies": {
    "@electron/get": "^2.0.2",
    "@wdio/logger": "^8.1.0",
    "ci-info": "^3.7.1",
    "extract-zip": "^2.0.1",
    "import-meta-resolve": "^2.2.1",
    "wdio-chromedriver-service": "^8.0.1",
    "webdriverio": "^8.1.3"
  },
  "devDependencies": {
    "@types/chromedriver": "^81.0.1",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "@wdio/cli": "^8.1.3",
    "@wdio/types": "^8.1.2",
    "electron": "^22.0.2",
    "eslint": "^8.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-wdio": "^8.0.14",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "release-it": "^15.6.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4",
    "uuid": "^9.0.0"
  },
  "files": [
    "bin/*",
    "dist/*",
    "main.js",
    "preload.js"
  ],
  "lint-staged": {
    "**/*.{js,ts}": [
      "eslint --fix"
    ],
    "**/*.{yml,json,md,ts,js}": [
      "prettier --write"
    ]
  }
}
